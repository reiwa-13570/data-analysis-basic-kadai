      "source": [
        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1],\n",
        "              [2, 3],\n",
        "              [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3],\n",
        "              [4, 5, 6, 7]])\n",
        "result = np.dot(A, B)\n",
        "\n",
        "print(\"行列の積の計算\")\n",
        "print(result)\n",
        "\n",
        "max_value = np.max(result)\n",
        "print(\"計算された行列の要素の最大値:\", max_value)"
      ]
    }
  ]
